                            <!-- Login Button -->
                            <div class="book-now-btn ml-3 ml-lg-5">
                                <% if(loginState){ %>
                                <a href="#" id="logout_btn">로그아웃 <i class="fa fa-long-arrow-right"
                                        aria-hidden="true"></i></a>
                                <% }else{ %>
                                <a id="login_page_btn" data-toggle="modal" data-target="#modalLRForm">로그인 <i
